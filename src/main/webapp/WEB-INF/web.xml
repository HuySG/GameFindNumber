<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>player</servlet-name>
    <servlet-class>Model.Player</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>homeServlet</servlet-name>
    <servlet-class>Servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>gameServlet</servlet-name>
    <servlet-class>Servlet.GameServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>gamectroller</servlet-name>
    <servlet-class>Controller.GameController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>gameRecord</servlet-name>
    <servlet-class>Model.GameRecord</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>player</servlet-name>
    <url-pattern>/player</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>homeServlet</servlet-name>
    <url-pattern>/homeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>gameServlet</servlet-name>
    <url-pattern>/gameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>gamectroller</servlet-name>
    <url-pattern>/gamectroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>gameRecord</servlet-name>
    <url-pattern>/gameRecord</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
  <welcome-file>homeServlet</welcome-file>
</welcome-file-list>
</web-app>
